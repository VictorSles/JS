{
    "manifest_version": 2,
    "name": "Auto scroll",
    "version": "0.1",
    "icons":{
        "16": "1-16.png",
        "24": "1-24.png",
        "32": "1-32.png",
        "48": "1-48.png",
        "128":"1-128.png"
    },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "./script/content.js"
            ]
        }
    ],
    "browser_action":{
        "default_icon": "1.png",
        "default_popup": "popup.html"
    },
    "permissions":[
        "activelab"
    ]
}

